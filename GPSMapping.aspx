    <script src="https://unpkg.com/leaflet@1.9.4/dist/leaflet.js" type="text/javascript"></script>
    <script type="text/javascript">
        var map;
        var markers = [];
        var pathLine = null;
        var allData = [];
        var highlightedMarkers = [];

        function initMap() {
            map = L.map('map').setView([35.6762, 139.6503], 10);
            
            L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
                attribution: '¬© OpenStreetMap contributors'
            }).addTo(map);
        }

        function clearMarkers() {
            markers.forEach(function(marker) {
                map.removeLayer(marker);
            });
            markers = [];
            if (pathLine) {
                map.removeLayer(pathLine);
                pathLine = null;
            }
        }

        function getColorByIndex(index, total) {
            var hue = (index / Math.max(total - 1, 1)) * 240;
            return 'hsl(' + (240 - hue) + ', 70%, 50%)';
        }

        function parseCSVLine(line) {
            var result = [];
            var current = '';
            var inQuotes = false;
            
            for (var i = 0; i < line.length; i++) {
                var char = line.charAt(i);
                
                if (char === '"') {
                    inQuotes = !inQuotes;
                } else if (char === ',' && !inQuotes) {
                    result.push(current.trim());
                    current = '';
                } else {
                    current += char;
                }
            }
            result.push(current.trim());
            return result;
        }

        function parseTimestamp(timestampStr) {
            if (!/^\d{12}$/.test(timestampStr)) {
                throw new Error('Êó•ÊôÇ„ÅØ12Ê°Å„ÅÆÊï∞Â≠ó (YYYYMMDDhhmm) „Åß„ÅÇ„ÇãÂøÖË¶Å„Åå„ÅÇ„Çä„Åæ„Åô: ' + timestampStr);
            }
            
            var year = timestampStr.substring(0, 4);
            var month = timestampStr.substring(4, 6);
            var day = timestampStr.substring(6, 8);
            var hour = timestampStr.substring(8, 10);
            var minute = timestampStr.substring(10, 12);
            
            var date = new Date(year, month - 1, day, hour, minute);
            var formatted = year + 'Âπ¥' + month + 'Êúà' + day + 'Êó• ' + hour + ':' + minute;
            
            return { date: date, formatted: formatted };
        }

        function parseCSV(csvText) {
            var lines = csvText.trim().split('\n');
            var data = [];
            
            for (var i = 0; i < lines.length; i++) {
                var line = lines[i].trim();
                if (!line) continue;
                
                var columns = parseCSVLine(line);
                if (columns.length < 4) {
                    throw new Error('Ë°å ' + (i + 1) + ': ÂàóÊï∞„Åå‰∏çË∂≥„Åó„Å¶„ÅÑ„Åæ„Åô (ÂøÖË¶Å: 4Âàó, ÂÆüÈöõ: ' + columns.length + 'Âàó)');
                }
                
                var id = columns[0].replace(/"/g, '').trim();
                var latStr = columns[1].replace(/"/g, '').trim();
                var lngStr = columns[2].replace(/"/g, '').trim();
                var timestampStr = columns[3].replace(/"/g, '').trim();
                
                if (!/^[A-Za-z]\d{6}$|^\d{7}$/.test(id)) {
                    throw new Error('Ë°å ' + (i + 1) + ': ID "' + id + '" „ÅØ7Ê°ÅÔºàÊï∞Â≠ó„ÅÆ„ÅøÔºâ„Åæ„Åü„ÅØ7ÊñáÂ≠óÔºàÊúÄÂàù„Åå„Ç¢„É´„Éï„Ç°„Éô„ÉÉ„ÉàÔºã6Ê°ÅÊï∞Â≠óÔºâ„Åß„ÅÇ„ÇãÂøÖË¶Å„Åå„ÅÇ„Çä„Åæ„Åô');
                }
                
                var lat = parseFloat(latStr);
                var lng = parseFloat(lngStr);
                
                if (isNaN(lat)) {
                    throw new Error('Ë°å ' + (i + 1) + ': Á∑ØÂ∫¶ "' + latStr + '" „ÅåÊï∞ÂÄ§„Åß„ÅØ„ÅÇ„Çä„Åæ„Åõ„Çì');
                }
                if (isNaN(lng)) {
                    throw new Error('Ë°å ' + (i + 1) + ': ÁµåÂ∫¶ "' + lngStr + '" „ÅåÊï∞ÂÄ§„Åß„ÅØ„ÅÇ„Çä„Åæ„Åõ„Çì');
                }
                
                if (lat < -90 || lat > 90) {
                    throw new Error('Ë°å ' + (i + 1) + ': Á∑ØÂ∫¶ ' + lat + ' „ÅåÊúâÂäπÁØÑÂõ≤Â§ñ„Åß„Åô (-90„Äú90)');
                }
                if (lng < -180 || lng > 180) {
                    throw new Error('Ë°å ' + (i + 1) + ': ÁµåÂ∫¶ ' + lng + ' „ÅåÊúâÂäπÁØÑÂõ≤Â§ñ„Åß„Åô (-180„Äú180)');
                }
                
                try {
                    var timestampInfo = parseTimestamp(timestampStr);
                    data.push({ 
                        id: id, 
                        timestamp: timestampInfo.date, 
                        timestampFormatted: timestampInfo.formatted,
                        lat: lat, 
                        lng: lng 
                    });
                } catch (error) {
                    throw new Error('Ë°å ' + (i + 1) + ': ' + error.message);
                }
            }
            
            return data;
        }

        function loadCSVData(data) {
            if (data.length === 0) {
                showMessage('„Éá„Éº„Çø„ÅåË¶ã„Å§„Åã„Çä„Åæ„Åõ„Çì„Åß„Åó„Åü', 'error');
                return;
            }
            
            allData = data.slice();
            allData.sort(function(a, b) {
                return a.timestamp - b.timestamp;
            });
            
            showMessage('‚úÖ ' + data.length + '‰ª∂„ÅÆCSV„Éá„Éº„Çø„ÇíË™≠„ÅøËæº„Åø„Åæ„Åó„Åü„ÄÇÊ§úÁ¥¢„Åó„Å¶„Åè„Å†„Åï„ÅÑ„ÄÇ', 'success');
            showFileInfo('Ë™≠„ÅøËæº„ÅøÂÆå‰∫Ü: ' + data.length + '‰ª∂„ÅÆ„Éá„Éº„ÇøÔºàÊ§úÁ¥¢ÂèØËÉΩÔºâ');
            
            document.getElementById('searchContainer').style.display = 'block';
        }

        function displayDataOnMap(data) {
            clearMarkers();
            
            if (data.length === 0) {
                showMessage('Ë°®Á§∫„Åô„Çã„Éá„Éº„Çø„Åå„ÅÇ„Çä„Åæ„Åõ„Çì', 'error');
                return;
            }
            
            if (data.length > 200) {
                showMessage('‚ö†Ô∏è Ê§úÁ¥¢ÁµêÊûú„Åå200‰ª∂„ÇíË∂Ö„Åà„Å¶„ÅÑ„Åæ„ÅôÔºà' + data.length + '‰ª∂Ôºâ„ÄÇÊù°‰ª∂„ÇíÁµû„ÇäËæº„Çì„ÅßÂÜçÊ§úÁ¥¢„Åó„Å¶„Åè„Å†„Åï„ÅÑ„ÄÇ', 'error');
                document.getElementById('searchResult').textContent = 'Ê§úÁ¥¢ÁµêÊûú„ÅåÂ§ö„Åô„Åé„Åæ„Åô„ÄÇÊù°‰ª∂„ÇíÁµû„ÇäËæº„Çì„Åß„Åè„Å†„Åï„ÅÑÔºà' + data.length + '‰ª∂Ôºâ';
                return;
            }
            
            data.sort(function(a, b) {
                return a.timestamp - b.timestamp;
            });
            
            var bounds = L.latLngBounds();
            var pathCoordinates = [];
            
            data.forEach(function(point, index) {
                var orderNumber = index + 1;
                var color = getColorByIndex(index, data.length);
                
                var customIcon = L.divIcon({
                    html: '<div class="custom-div-icon" style="background-color: ' + color + ';">' + orderNumber + '</div>',
                    iconSize: [30, 30],
                    iconAnchor: [15, 15],
                    popupAnchor: [0, -15],
                    className: 'custom-marker'
                });
                
                var marker = L.marker([point.lat, point.lng], { icon: customIcon })
                    .addTo(map)
                    .bindPopup(
                        '<strong>È†ÜÂ∫è:</strong> ' + orderNumber + 'Áï™ÁõÆ<br/>' +
                        '<strong>ID:</strong> ' + point.id + '<br/>' +
                        '<strong>GPSÂèñÂæóÊôÇÂàª:</strong> ' + point.timestampFormatted + '<br/>' +
                        '<strong>‰ΩçÁΩÆ:</strong> ' + point.lat.toFixed(6) + ', ' + point.lng.toFixed(6)
                    );
                
                marker._pointData = point;
                markers.push(marker);
                bounds.extend([point.lat, point.lng]);
                pathCoordinates.push([point.lat, point.lng]);
            });
            
            if (pathCoordinates.length > 1) {
                pathLine = L.polyline(pathCoordinates, {
                    color: '#0078d4',
                    weight: 3,
                    opacity: 0.7,
                    dashArray: '5, 10'
                }).addTo(map);
            }
            
            if (data.length > 0) {
                map.fitBounds(bounds, { padding: [20, 20] });
            }
        }

        function showMessage(message, type) {
            type = type || 'info';
            var messageDiv = document.getElementById('message');
            messageDiv.innerHTML = message;
            messageDiv.className = type;
            messageDiv.style.display = 'block';
        }

        function showFileInfo(info) {
            var infoDiv = document.getElementById('fileInfo');
            infoDiv.textContent = info;
            infoDiv.style.display = 'block';
        }

        function handleFileSelect(event) {
            var file = event.target.files[0];
            if (!file) return;
            
            showFileInfo('üìÑ „Éï„Ç°„Ç§„É´Ë™≠„ÅøËæº„Åø‰∏≠: ' + file.name);
            
            var reader = new FileReader();
            reader.onload = function(e) {
                try {
                    var csvText = e.target.result;
                    var data = parseCSV(csvText);
                    loadCSVData(data);
                } catch (error) {
                    showMessage('‚ùå „Ç®„É©„Éº: ' + error.message, 'error');
                    document.getElementById('fileInfo').style.display = 'none';
                }
            };
            
            reader.onerror = function() {
                showMessage('‚ùå „Éï„Ç°„Ç§„É´„ÅÆË™≠„ÅøËæº„Åø„Å´Â§±Êïó„Åó„Åæ„Åó„Åü', 'error');
                document.getElementById('fileInfo').style.display = 'none';
            };
            
            reader.readAsText(file, 'UTF-8');
        }

        function clearHighlight() {
            highlightedMarkers.forEach(function(marker) {
                var element = marker.getElement();
                if (element) {
                    element.classList.remove('highlight-marker');
                }
            });
            highlightedMarkers = [];
        }

        function highlightMarkers(matchedData) {
            clearHighlight();
            
            markers.forEach(function(marker) {
                var pointData = marker._pointData;
                var isMatched = matchedData.some(function(data) {
                    return data.id === pointData.id && 
                           data.timestamp.getTime() === pointData.timestamp.getTime();
                });
                
                if (isMatched) {
                    var element = marker.getElement();
                    if (element) {
                        element.classList.add('highlight-marker');
                        highlightedMarkers.push(marker);
                    }
                }
            });
        }

        function searchById() {
            var searchId = document.getElementById('searchId').value.trim();
            var resultDiv = document.getElementById('searchResult');
            
            if (!searchId) {
                resultDiv.textContent = 'ID„ÇíÂÖ•Âäõ„Åó„Å¶„Åè„Å†„Åï„ÅÑ';
                return;
            }
            
            if (!/^[A-Za-z]\d{6}$|^\d{7}$/.test(searchId)) {
                resultDiv.textContent = '7Ê°Å„ÅÆÊï∞Â≠ó„ÄÅ„Åæ„Åü„ÅØÊúÄÂàù„Åå„Ç¢„É´„Éï„Ç°„Éô„ÉÉ„ÉàÔºã6Ê°ÅÊï∞Â≠ó„ÇíÂÖ•Âäõ„Åó„Å¶„Åè„Å†„Åï„ÅÑ';
                return;
            }
            
            var matchedData = allData.filter(function(point) {
                return point.id === searchId;
            });
            
            if (matchedData.length === 0) {
                resultDiv.textContent = '‚ùå ID "' + searchId + '" „ÅåË¶ã„Å§„Åã„Çä„Åæ„Åõ„Çì„Åß„Åó„Åü';
                clearMarkers();
            } else if (matchedData.length > 200) {
                resultDiv.textContent = '‚ö†Ô∏è ID "' + searchId + '" „ÅÆÊ§úÁ¥¢ÁµêÊûú„Åå200‰ª∂„ÇíË∂Ö„Åà„Å¶„ÅÑ„Åæ„ÅôÔºà' + matchedData.length + '‰ª∂Ôºâ„ÄÇÊù°‰ª∂„ÇíÁµû„ÇäËæº„Çì„Åß„Åè„Å†„Åï„ÅÑ';
                clearMarkers();
            } else {
                resultDiv.textContent = '‚úÖ ID "' + searchId + '" „Åß ' + matchedData.length + ' ‰ª∂Ë¶ã„Å§„Åã„Çä„Åæ„Åó„Åü';
                displayDataOnMap(matchedData);
                
                if (matchedData.length === 1) {
                    map.setView([matchedData[0].lat, matchedData[0].lng], 15);
                }
            }
        }

        function searchByDate() {
            var searchDate = document.getElementById('searchDate').value.trim();
            var resultDiv = document.getElementById('searchResult');
            
            if (!searchDate) {
                resultDiv.textContent = 'Êó•‰ªò„ÇíÂÖ•Âäõ„Åó„Å¶„Åè„Å†„Åï„ÅÑ';
                return;
            }
            
            if (!/^\d{8}$/.test(searchDate)) {
                resultDiv.textContent = 'YYYYMMDDÂΩ¢Âºè„ÅßÂÖ•Âäõ„Åó„Å¶„Åè„Å†„Åï„ÅÑÔºà‰æã: 20240115Ôºâ';
                return;
            }
            
            var matchedData = allData.filter(function(point) {
                var timestampStr = point.timestamp.getFullYear().toString() +
                    ('0' + (point.timestamp.getMonth() + 1)).slice(-2) +
                    ('0' + point.timestamp.getDate()).slice(-2);
                return timestampStr === searchDate;
            });
            
            if (matchedData.length === 0) {
                resultDiv.textContent = '‚ùå Êó•‰ªò "' + searchDate + '" „ÅÆ„Éá„Éº„Çø„ÅåË¶ã„Å§„Åã„Çä„Åæ„Åõ„Çì„Åß„Åó„Åü';
                clearMarkers();
            } else if (matchedData.length > 200) {
                resultDiv.textContent = '‚ö†Ô∏è Êó•‰ªò "' + searchDate + '" „ÅÆÊ§úÁ¥¢ÁµêÊûú„Åå200‰ª∂„ÇíË∂Ö„Åà„Å¶„ÅÑ„Åæ„ÅôÔºà' + matchedData.length + '‰ª∂Ôºâ„ÄÇÊù°‰ª∂„ÇíÁµû„ÇäËæº„Çì„Åß„Åè„Å†„Åï„ÅÑ';
                clearMarkers();
            } else {
                resultDiv.textContent = '‚úÖ Êó•‰ªò "' + searchDate + '" „Åß ' + matchedData.length + ' ‰ª∂Ë¶ã„Å§„Åã„Çä„Åæ„Åó„Åü';
                displayDataOnMap(matchedData);
                
                if (matchedData.length > 0) {
                    var bounds = L.latLngBounds();
                    matchedData.forEach(function(point) {
                        bounds.extend([point.lat, point.lng]);
                    });
                    map.fitBounds(bounds, { padding: [20, 20] });
                }
            }
        }

        function clearSearch() {
            document.getElementById('searchId').value = '';
            document.getElementById('searchDate').value = '';
            document.getElementById('searchResult').textContent = '';
            clearMarkers();
        }

        // SharePointÂØæÂøú„ÅÆÂàùÊúüÂåñ
        function initializePage() {
            initMap();
            document.getElementById('csvFile').addEventListener('change', handleFileSelect);
            
            document.getElementById('searchId').addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    e.preventDefault();
                    searchById();
                }
            });
            
            document.getElementById('searchDate').addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    e.preventDefault();
                    searchByDate();
                }
            });
        }

        // SharePointÁí∞Â¢É„Åß„ÅÆÂàùÊúüÂåñ
        if (typeof _spPageContextInfo !== 'undefined') {
            // SharePointÁí∞Â¢É
            ExecuteOrDelayUntilScriptLoaded(initializePage, "sp.js");
        } else {
            // ÈÄöÂ∏∏„ÅÆ„Éñ„É©„Ç¶„Ç∂Áí∞Â¢É
            document.addEventListener('DOMContentLoaded', initializePage);
        }
    </script>
</body>
